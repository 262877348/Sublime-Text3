    <content><![CDATA[<if condition="${1:(\$name eq 1) OR (\$name gt 100)}"> 
	value1
<elseif condition="${2:(\$name eq 2)}"/>
	value2
<else /> 
	value3
</if>
]]></content>
    <tabTrigger>tpifelse</tabTrigger>
    <scope>text.html</scope>
    <description>Tp_tpifelse</description>
</snippet>
